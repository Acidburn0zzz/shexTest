@prefix ex: <http://a.example> .
@prefix sx: <http://shex.io/ns/shex#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://a.example/S1> a sx:Shape ;
    sx:expression [ a sx:TripleConstraint ;
            sx:predicate <http://a.example/p1> ;
            sx:valueExpr [ a sx:ShapeRef ;
                    sx:reference _:b3 ] ] .

[] a sx:Schema ;
    sx:shapes _:b3,
        <http://a.example/S1> .

_:b3 a sx:NodeConstraint ;
    sx:minlength "5"^^xsd:nonNegativeInteger ;
    sx:nodeKind "literal" .


