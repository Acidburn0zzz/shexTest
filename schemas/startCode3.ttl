    sx:startActs ([
      a sx:SemAct;
      sx:code " print(\"startAct 1\") ";
      sx:name <http://shex.io/extensions/Test/>
    ] [
      a sx:SemAct;
      sx:code " print(\"startAct 2\") ";
      sx:name <http://shex.io/extensions/Test/>
    ] [
      a sx:SemAct;
      sx:code " print(\"startAct 3\") ";
      sx:name <http://shex.io/extensions/Test/>
    ]) .


